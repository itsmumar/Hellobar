{{{hellobar_base_js}}}

if (Object.prototype.toString.call( _hbq ) === "[object Array]") {
  HB = _HB;
  {{#templates}}
    HB.setTemplate("{{name}}", {{{markup}}});
  {{/templates}}

  HB.addCSS({{{hellobar_base_css}}});
  HB.extCSS = {{{hellobar_container_css}}};
}

{{#rules}}
  _hbq.push(function(){HB.addRule("{{match}}", {{{conditions}}}, {{{site_elements}}})});
{{/rules}}

_hbq = new HBQ();
