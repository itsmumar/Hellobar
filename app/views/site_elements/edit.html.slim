#ember-root

javascript:
  window.barID = #{@site_element.id};
  window.siteID = #{@site.id};
  window.rules = #{{@rules.map{|rule| RuleSerializer.new(rule) }.to_json }};
  window.targetingUiVariant = #{(ab_variation_or_nil("Targeting UI Variation 2016-06-13") || "") == 'variant'};
  window.HB_DATA = {
    skipInterstitial: true
  }
  window.siteTimezone = "#{@site.timezone}"
