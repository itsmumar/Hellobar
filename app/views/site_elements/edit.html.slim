#ember-root

javascript:
  var barID = #{@site_element.id};
  var siteID = #{@site.id};
  var rules = #{{@rules.map{|rule| RuleSerializer.new(rule) }.to_json }};
  window.targetingUiVariant = #{(get_ab_variation_or_nil("Targeting UI Variation 2016-06-13") || "") == 'variant'};
  window.HB_DATA = {
    onboardingLimitedVariant: #{false and get_ab_variation("Onboarding Limitted To Three Goals 2016-05-11") == 'variant'},
    skipInterstitial: #{not render_interstitial?}
  }