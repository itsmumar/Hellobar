#ember-root

javascript:
  window.siteID = #{@site.id}
  window.rules = #{{@rules.map{|rule| RuleSerializer.new(rule) }.to_json }};
  window.targetingUiVariant = #{(get_ab_variation_or_nil("Targeting UI Variation 2016-06-13") || "") == 'variant'};
  window.HB_DATA = {
    skipInterstitial: #{not render_interstitial?}
  }

- unless params[:element_to_copy_id].blank?
  javascript:
    window.elementToCopyID = #{params[:element_to_copy_id]}
