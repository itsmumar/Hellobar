@charset 'UTF-8'

/*-----------  Weight Corrections  -----------*/

@mixin antialiased($smooth: true)
  @if $smooth
    @include prefixer(font-smoothing, antialiased, webkit moz ms o spec)
  @else
    @include prefixer(font-smoothing, subpixel-antialiased, webkit moz ms o spec)

/*-----------  Animations  -----------*/

@include keyframes(spin)
  from
    @include transform(rotate(0deg))
  to 
    @include transform(rotate(-360deg))

/*-----------  Custom Icon Font  -----------*/

@font-face
  font-family: "hellobar"
  src: url(<%= asset_path "hellobar.eot" %>)
  src: url(<%= asset_path "hellobar.eot?#iefix" %>) format("embedded-opentype"), url(<%= asset_path "hellobar.woff" %>) format("woff"), url(<%= asset_path "hellobar.ttf" %>) format("truetype"), url(<%= asset_path "hellobar.svg#hellobar" %>) format("svg")
  font-weight: normal
  font-style: normal

[data-icon]:before
  font-family: 'hellobar' !important
  content: attr(data-icon)
  font-style: normal !important
  font-weight: normal !important
  font-variant: normal !important
  text-transform: none !important
  speak: none
  line-height: 1
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

[class^='icon-']:before, [class*=' icon-']:before
  font-family: 'hellobar' !important
  font-style: normal !important
  font-weight: normal !important
  font-variant: normal !important
  text-transform: none !important
  speak: none
  line-height: 1
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

